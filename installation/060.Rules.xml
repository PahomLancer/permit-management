    v_project_id number;
begin
    select finish_projected_date,
           finish_actual_date,
           xitor_id
      into v_fp_date,
           v_fa_date,
           v_project_id
      from wp_tasks
     where wp_task_id = :wp_task_id;
    for rec in (
        select child_id
          from relation
         where relation_type_id = id.project.rel.permit
           and parent_id = v_project_id
    ) loop
        permgmt_permit_managment.calc_permit_expires_before_cx_s(rec.child_id, v_fa_date, v_fp_date);
    end loop;
end;</SQL_TEXT>
