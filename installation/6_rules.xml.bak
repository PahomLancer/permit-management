  <SQL_TEXT>declare
    v_fa_date date;
    v_fp_date date;
    v_project_id number;
begin
    v_project_id := util.getparentbyid(:pk, id.project.tt);
    select finish_projected_date,
           finish_actual_date
      into v_fp_date,
           v_fa_date
      from wp_tasks
     where xitor_id = v_project_id 
       and order_number = 7000 
       and rownum = 1;
    pkg_permit_managment.calc_permit_expires_before_cx_s(:pk, v_fa_date, v_fp_date);
exception when no_data_found then
    util.setvalnumbyid(:pk, id.permits.cf.prm_permit_expires_before_cx_s, 0);
    return;
end;</SQL_TEXT>
